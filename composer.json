{
    "name": "kenned/php-template",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "twig/twig": "^3.0",
        "ext-pdo": "*",
        "twilio/sdk": "^8.3",
        "stripe/stripe-php": "^16.6",
        "ext-curl": "*",
        "ralouphie/mimey": "^1.0"
    },
    "autoload": {
        "psr-4": {
